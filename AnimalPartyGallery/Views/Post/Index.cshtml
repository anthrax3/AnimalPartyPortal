@model IEnumerable<AnimalPartyGallery.Models.Post>


<h2>@ViewBag.Title</h2>
<div class="index-border-radius">
    <table class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>Date</th>
                <th>Event Name</th>
                <th>Producer</th>
                <th>Phone</th>
                <th>Album</th>
                <th>Hitchhikers</th>
            </tr>
        </thead>
        <tbody>
         @if(Model!=null){
            foreach (var item in Model)
            {
            <tr class="danger">
                <td>@Html.DisplayFor(modelItem => item.Date)</td>
                <td>@Html.DisplayFor(modelItem => item.Title)</td>
                <td>@Html.DisplayFor(modelItem => item.Producer)</td>
                <td>@Html.DisplayFor(modelItem => item.PhoneNumber)</td>
                <td>@Html.ActionLink("Link", "Details", "Post", new { id = item.ID }, null)</td>
                <td>@Html.ActionLink("Link", "Index", "Hitchhiker", new { id = item.ID }, null)</td>
            </tr>
          }
            }
            

        </tbody>
    </table>
</div>




